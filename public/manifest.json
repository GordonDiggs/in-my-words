{
  "name" : "Chrome Extension Skeleton",
  "version" : "0.0.1",
  "description" : "Whatever you want it to be",
  "background_page" : "views/background.html",
  "content_scripts" : [
    {
      "matches" : [
        "http://*/*",
        "https://*/*"
      ],
      "js" : ["assets/application.js"],
      "run_at" : "document_start",
      "all_frames" : true
    }
  ],
  "options_page": "views/options.html",
  "icons" : {
    "48" : "icon/icon-48.png",
    "128" : "icon/icon-128.png"
  }
}
